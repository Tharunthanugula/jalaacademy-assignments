# 1. Print your name
print("Tharun Thanugula")

 #2. Single line and multi-line comments
# This is a single-line comment

'''
This is a
multi-line
comment
'''

# 3. Variables of different data types
a = 10            # int
b = True          # boolean
c = 'T'           # char (single character string)
d = 3.14          # float
e = 9.87654321    # double (in Python, float handles double precision)

print(a, b, c, d, e)

# 4. Local and Global Variables
x = "Global Variable"

def demo_scope():
    x = "Local Variable"
    print("Inside function:", x)

demo_scope()
print("Outside function:", x)
